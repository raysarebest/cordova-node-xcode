{
  "author": "Apache Software Foundation",
  "name": "xcode",
  "description": "parser for xcodeproj/project.pbxproj files",
  "version": "3.0.2-dev",
  "main": "index.js",
  "repository": "github:apache/cordova-node-xcode",
  "bugs": "https://github.com/apache/cordova-node-xcode/issues",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "simple-plist": "^1.4.0-0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.9",
    "@types/node": "^18.0.6",
    "@types/pegjs": "^0.10.3",
    "jsdoc": "^3.6.10",
    "nodeunit": "^0.11.3",
    "nyc": "^15.0.0",
    "pegjs": "dev",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "pegjs": "node_modules/.bin/pegjs --extra-options '{\"input\": \"lib/parser/pbxproj.pegjs\", \"output\": \"lib/parser/pbxproj.js\", \"header\": \"// @ts-nocheck\"}'",
    "test": "npm run cover",
    "test:unit": "nodeunit test/parser test",
    "cover": "nyc npm run test:unit",
    "typescript": "npx tsc utils/*.ts; node utils/updateBuildSettingsTypes.js; npx tsc -p jsconfig.json; rsync -a --prune-empty-dirs --include '*/' --include '*.d.ts' --exclude '*' lib/ types/lib/",
    "prepack": "npm run typescript"
  },
  "license": "Apache-2.0",
  "types": "types/index.d.ts",
  "nyc": {
    "all": true,
    "exclude": [
      "coverage/",
      "test/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
